<?xml version="1.0" encoding="utf-8"?>
<schema targetNamespace="XMLSpace" xmlns="http://www.w3.org/2001/XMLSchema">
  <element name="space">
    <complexType>
      <sequence>
        <element minOccurs="1" maxOccurs="unbounded" name="element_data">
          <complexType>
            <attribute name="element_id" type="integer" use="required" />
            <attribute name="order" type="integer" use="required" />
            <attribute name="bdof" type="integer" use="required" />
            <attribute name="n" type="integer" use="required" />
            <attribute name="changed_in_last_adaptation" type="boolean" use="required" />
          </complexType>
        </element>
      </sequence>
	  <attribute name="spaceType" type="string"/>
    </complexType>
  </element>
</schema>
