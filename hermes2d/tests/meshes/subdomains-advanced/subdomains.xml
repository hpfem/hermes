<?xml version="1.0" encoding="utf-8"?>
<domain:domain xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:domain="XMLSubdomains"
	xmlns:element="XMLMesh"
	xsi:schemaLocation="XMLSubdomains ../../../include/mesh/subdomains_h2d_xml.xsd">
  <vertices>
    <vertex x="0" y="0" />
    <vertex x="6" y="0" />
    <vertex x="6" y="6" />
    <vertex x="0" y="6" />
    <vertex x="2" y="2" />
    <vertex x="4" y="2" />
    <vertex x="4" y="4" />
    <vertex x="2" y="4" />
  </vertices>

  <elements>
    <element:quad v1="0" v2="1" v3="5" v4="4" marker="Outside" />
    <element:quad v1="1" v2="2" v3="6" v4="5" marker="Outside" />
    <element:quad v1="2" v2="3" v3="7" v4="6" marker="Outside" />
    <element:quad v1="3" v2="0" v3="4" v4="7" marker="Outside" />
    <element:quad v1="4" v2="5" v3="6" v4="7" marker="Inner Circle" />
	</elements>

  <boundary_edges>
    <boundary_edge v1="0" v2="1" marker="Outer" />
		<boundary_edge v1="1" v2="2" marker="Outlet" />
		<boundary_edge v1="2" v2="3" marker="Outer" />
		<boundary_edge v1="3" v2="0" marker="Inlet" />
    
    <boundary_edge v1="4" v2="5" marker="Inner" />
		<boundary_edge v1="5" v2="6" marker="Inner" />
		<boundary_edge v1="6" v2="7" marker="Inner" />
		<boundary_edge v1="7" v2="4" marker="Inner" />
	</boundary_edges> 
  
  <curves>
    <arc v1="4" v2="5" angle="90" />
		<arc v1="5" v2="6" angle="90" />
    <arc v1="6" v2="7" angle="90" />
		<arc v1="7" v2="4" angle="90" />
  </curves>
  
	<subdomains>
		<subdomain name="Whole domain">
      <boundary_edge_number number="0" />
      <boundary_edge_number number="1" />
			<boundary_edge_number number="2" />
			<boundary_edge_number number="3" />
    </subdomain>
		<subdomain name="Domain without the inner circle">
			<element_number number="0" />
      <element_number number="1" />
      <element_number number="2" />
      <element_number number="3" />
		</subdomain>
	</subdomains>
</domain:domain>